{
  "name": "GAS Prettier",
  "description": "Google Apps Script Prettier Formatter Chrome Extention",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "host_permissions": ["https://script.google.com/home/projects/*/edit"],
  "icons": {
    "48": "/img/apps_script_1x_48dp.png"
  },
  "content_scripts": [
    {
      "matches": ["https://script.google.com/home/projects/*/edit"],
      "js": [
        "src/lib/prettier/standalone.js",
        "src/lib/prettier/parser-babel.js",
        "src/lib/prettier/parser-html.js",
        "src/loadScript.js"
      ],
      "css": ["src/style.css"],
      "world": "MAIN"
    }
  ]
}
