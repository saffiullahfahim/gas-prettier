{
  "name": "GAS Prettier + AI Completion",
  "description": "Google Apps Script Prettier Formatter Chrome Extention with AI Completion",
  "version": "1.2.3",
  "manifest_version": 3,
  "host_permissions": [
    "https://script.google.com/*",
    "https://labs.perplexity.ai/"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://script.google.com/*"],
      "js": [
        "src/lib/prettier/standalone.js",
        "src/lib/prettier/parser-babel.js",
        "src/lib/prettier/parser-html.js",
        "src/loadScript.js"
      ],
      "css": ["src/style.css"],
      "world": "MAIN"
    },
    {
      "matches": ["https://labs.perplexity.ai/"],
      "js": ["src/lib/socket.io/index.min.js", "src/pplxLoad.js"],
      "world": "MAIN",
      "all_frames": true
    }
  ]
}
